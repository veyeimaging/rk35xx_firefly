&pinctrl {
    touch {
        
        touch_int1: touch-int1 {
            rockchip,pins = <4 RK_PA4 RK_FUNC_GPIO &pcfg_pull_up>;
        };
        touch_int0: touch-int0 {
            rockchip,pins = <1 RK_PA7 RK_FUNC_GPIO &pcfg_pull_up>;
        };
    };
};

