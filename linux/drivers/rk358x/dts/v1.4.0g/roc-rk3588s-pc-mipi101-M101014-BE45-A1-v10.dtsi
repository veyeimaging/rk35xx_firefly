&pwm13 {
    pinctrl-0 = <&pwm13m1_pins>;
    status = "okay";
};

/*
 * mipi_dcphy1 needs to be enabled
 * when dsi1 is enabled
 */
&mipi_dcphy1 {
    status = "okay";
};

&dsi1_in_vp2 {
    status = "disabled";
};

&dsi1_in_vp3 {
    status = "okay";
};

&route_dsi1 {
    status = "okay";
    connect = <&vp3_out_dsi1>;
};

&route_hdmi0{
    status = "okay";
};
/*
&lcd_rst{
	status = "okay";
